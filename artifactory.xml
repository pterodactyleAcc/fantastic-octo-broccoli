<settings>
    <mirrors>
    		<mirror>
      			<id>repo1</id>
			<name>Accolade's artifactory to use as comapany repo</name>
			<url>${ARTIFACTORYKEY_URL}/accolade/repo1-cache</url>
			<mirrorOf>*</mirrorOf>
		</mirror>
	</mirrors>
    <servers>
        <server>
            <id>repo1</id>
            <username>${ARTIFACTORYKEY_USERNAME}</username>
            <password>${ARTIFACTORYKEY_PASSWORD}</password>
        </server>
    </servers>
</settings>
